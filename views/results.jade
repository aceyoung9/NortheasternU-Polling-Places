extends plainlayout

block content
  .results
    .row 
      .col-sm-12
        h1 Hello #{dormName} Resident!
        iframe(width="100%" height="350" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/directions?origin=#{address}&destination=#{pollingAddress}&mode=walking&key=AIzaSyBkU33xVkn78h4jXLr1TXtrv6dY27eHZSk")
    .row
      .col-sm-6
        h3 Your Polling Place
        address #{pollingLoc}
          br
          | #{pollingAddress}
        h3 Extra instructions
        p #{instructions}
        
      .col-sm-6
        h3 Details
        | Ward: #{wardNo}
        br
        | Precinct: #{precinctNo}
